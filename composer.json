{
  "name": "lucasgiori/rate-limit-php",
  "type": "project",
  "autoload": {
    "psr-4": {
      "RateLimit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RateLimit\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Lucas Giori Cesconetto",
      "email": "lucasgioricesconetto@gmail.com"
    }
  ],
  "require": {
    "phpunit/phpunit": "^9.5",
    "predis/predis": "^2.0",
    "ext-redis": "*",
    "ext-memcached": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.7.1"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit  --testdox",
    "test-coverage": "./vendor/bin/phpunit --coverage-html",
    "php-cs": "./vendor/bin/phpcs --standard=PSR12 ./src",
    "php-cbf": "./vendor/bin/phpcbf -h"
  }
}
